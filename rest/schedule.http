Authorization: Bearer staff-access-token-here
Content-Type: application/json

{
  "date": "2024-01-15",
  "startTime": "09:00",
  "endTime": "10:00",
  "duration": 60,
  "capacity": 20,
  "status": "available",
  "description": "Morning tour slot"
}

